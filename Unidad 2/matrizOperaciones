#include <stdio.h>

#define FILAS 6
#define COLUMNAS 3

int main() {
    int matriz [FILAS] [COLUMNAS];
    for (int i = 0; i < FILAS; i++) {
        for(int j = 0; j < 2; j++){
            printf("Ingrese una palabra \n");
            scanf("%d", matriz[i][j]);
        }
    }

    printf("Recorriendo la matriz: \n");
    for (int i = 0; i < FILAS; i++) {
        for(int j = 0; j < COLUMNAS; j++){
            printf("%d", matriz [i][j]);
        }
        printf("\n");
    }
    return 0;
}